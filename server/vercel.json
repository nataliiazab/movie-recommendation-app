{
  "version": 2,
  "builds": [
    {
      "src": "server/src/api/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "client/package.json",
      "use": "@vercel/react"
    }
  ],
  "routes": [
    {
      "src": "/api/movies",
      "dest": "/server/src/api/movies.ts"
    },
    {
      "src": "/api",
      "dest": "/server/src/api/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/client/$1"
    }
  ]
}
